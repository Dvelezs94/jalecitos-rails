<tr>
  <td><%= link_to "#{verification_dashboard.user.slug}", user_path(verification_dashboard.user) %></td>
  <td><%= verification_dashboard.curp %></td>
  <td>
  <% verification_dashboard.identification_urls.each do |id| %>
    <%=  link_to icon("fas","eye"), id, target: :_blank  %>
  <% end %>
  </td>
  <td><%= link_to icon("fas","eye"), verification_dashboard.address.url, target: :_blank %></td>
  <td><%= link_to icon("fas","eye"), verification_dashboard.criminal_letter.url, target: :_blank %></td>
  <td><%= verification_dashboard.status %></td>
  <% if verification_dashboard.status == "pending" %>
  <td><%= link_to icon("fas","check"), approve_verification_path(verification_dashboard.id), title: "aceptar", class: "fa-2x", method: :put %></td>
  <td><%= link_to icon("fas","times"), deny_verification_path(verification_dashboard.id), title: "denegar", class: "fa-2x", :data => {:confirm => 'Seguro que quieres denegar?'}, method: :put %></td>
  <% end %>
</tr>
