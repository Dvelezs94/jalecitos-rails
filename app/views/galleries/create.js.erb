//one response given
ajaxRecieved();
//if is the last, hide loading
if (window.sentRequests == 0){
  $(".loading").hide();
}
//do the render
<% if @success %> //loads an image
  $(".current_images").append("<%= j render partial: 'galleries/image', collection: [@gig.images.last] %>");
<% elsif @next %> //clicks next form
  window.success = true;
<% else %>
  alert("<%= j @gig.errors[:images][0].html_safe %>");
<% end %>
