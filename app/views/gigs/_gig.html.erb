<div class="col-lg-2 col-md-4 col-sm-6 col-12 single_gig">
   <div class="get_pro_content">
      <%= image_tag image_display_helper(gig.image_url), width: "100%", height: 139 %>
      <div class="get_pro_content_container">
         <div class="gig_avatar_"><img src="https://s3.us-east-2.amazonaws.com/cdn.jalecitos.com/images/gig_avatr.png"><%= gig.user.alias %> </div>
         <br>
         <div class="gig_name_field">
           <%=gig.name%>
         </div>
         <br>
         <br>
         <div class="row">
            <div class="col-lg-6 gig_stars">
               <div  class="gig-rating"><i class="fa fa-star"></i>5.0 <span>(2)</span></div>
            </div>
            <div class="col-lg-6">
               <img src="https://s3.us-east-2.amazonaws.com/cdn.jalecitos.com/images/pro-badge-outline-2.0.1b29959.svg">
            </div>
         </div>
      </div>
      <!--  -->
      <hr>
      <!--  -->
      <div class="favourites_and_price">
         <div class="row">
            <div class="col-lg-4">
               <i class="fa fa-bars"></i>
               <i class="fa fa-heart"></i>
            </div>
            <div class="col-lg-8">
               <span>Desde</span><strong> <%= number_to_currency gig.packages.first.price, precision: 2 %></strong>
            </div>
         </div>
      </div>
   </div>
</div>
