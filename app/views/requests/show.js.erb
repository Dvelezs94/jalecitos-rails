<% name = "other_offer" %>
<% collection = instance_variable_get("@"+name+"s") %>
html_item = '[paginated-'+'<%= j name %>'+"s]";
$(html_item + " > nav").remove(); //this deletes pagination
$(html_item).append('<%= j render(@other_offers) %>');
<% if @other_offers.next_page %>
$(html_item).append('<%= j paginate(@other_offers) %>');
<% end %>


<% sleep 1 %>
