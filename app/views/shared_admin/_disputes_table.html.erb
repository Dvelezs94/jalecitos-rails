<h2>Disputas</h2>
<div class="table-responsive paginator_container">
  <table class="table table-striped table-sm">
    <thead>
      <tr>
        <th>Empleado</th>
        <th>Empleador</th>
        <th>Orden</th>
        <th>Precio</th>
        <th>Estado de Disputa</th>
        <th>Estado de Orden</th>
      </tr>
    </thead>
    <tbody class="paginated_disputes">
      <%= render partial: 'admins/dispute', collection: @disputes %>
    </tbody>
  </table>
  <%= paginate @disputes, param_name: :dispute,
   :params => {:current => :dispute} %>
</div>
