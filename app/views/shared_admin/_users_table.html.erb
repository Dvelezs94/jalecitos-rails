<div class="col-md-8">
  <h2>Users</h2>
  <div class="table-responsive">
    <table class="table table-striped table-sm">
      <thead>
        <tr>
          <th>Name</th>
          <th>Email</th>
          <th>Role</th>
        </tr>
      </thead>
      <tbody>
          <%= render partial: 'users/user_dashboard', collection: @users %>
      </tbody>
    </table>
    <%= paginate @users, param_name: :user_page %>
  </div>
</div>
